<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
  </head>
  <body>
    <h1><%= title %></h1>
    <p>Welcome to <%= title %></p>
    <ul>
      <% topics.forEach(function(topic) { %>
        <li><a href="<%=topic.href%>"><%=topic.name%></a></li>
      <% }) %>
    </ul>
  </body>
</html>
